{
  "name": "base-api",
  "private": true,
  "version": "1.0.0",
  "description": "API for base APP",
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/src/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "mocha --timeout 5000 --import=tsx  './test/**/*.spec.ts'",
    "test:watch": "mocha --timeout 5000 --watch --import=tsx  ./test/**/*.spec.ts",
    "test:cov": "nyc --reporter=lcov --reporter=cobertura mocha --timeout 5000 --reporter=cypress-multi-reporters --reporter-options configFile=cypress-multi-reporters.json --import=tsx  --recursive './test/**/*.spec.ts'",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/mocha --runInBand",
    "test:single": "mocha --import=tsx  ",
    "prepare": "node -e \"if (process.env.NODE_ENV !== 'production'){process.exit(1)} \" || husky install",
    "typeorm": "ts-node ./node_modules/typeorm/cli",
    "typeorm:run-migrations": "npm run typeorm migration:run -- -d ./typeOrm.config.ts",
    "typeorm:generate-migration": "npm run typeorm -- -d ./typeOrm.config.ts migration:generate ",
    "typeorm:create-migration": "npm run typeorm -- migration:create ",
    "typeorm:revert-migration": "npm run typeorm -- -d ./typeOrm.config.ts migration:revert"
  },
  "dependencies": {
    "@nestjs-modules/mailer": "2.0.2",
    "@nestjs/common": "11.1.5",
    "@nestjs/config": "4.0.2",
    "@nestjs/core": "11.1.5",
    "@nestjs/jwt": "11.0.0",
    "@nestjs/passport": "11.0.5",
    "@nestjs/platform-express": "11.1.5",
    "@nestjs/swagger": "11.2.0",
    "@nestjs/typeorm": "^11.0.0",
    "bcrypt": "6.0.0",
    "class-transformer": "0.5.1",
    "class-validator": "0.14.2",
    "jsonwebtoken": "9.0.2",
    "mysql2": "^3.14.3",
    "passport": "0.7.0",
    "passport-jwt": "4.0.1",
    "passport-local": "1.0.0",
    "pg": "8.16.3",
    "pgsql": "1.0.0",
    "rimraf": "6.0.1",
    "rxjs": "7.8.2",
    "typeorm": "^0.3.25"
  },
  "devDependencies": {
    "@nestjs/cli": "11.0.9",
    "@nestjs/schematics": "11.0.6",
    "@nestjs/testing": "11.1.5",
    "@types/bcrypt": "6.0.0",
    "@types/chai": "5.2.2",
    "@types/express": "5.0.3",
    "@types/jest": "30.0.0",
    "@types/mocha": "10.0.10",
    "@types/node": "24.1.0",
    "@types/passport-jwt": "4.0.1",
    "@types/passport-local": "1.0.38",
    "@types/sinon": "17.0.4",
    "@types/supertest": "6.0.3",
    "@typescript-eslint/eslint-plugin": "8.38.0",
    "@typescript-eslint/parser": "8.38.0",
    "chai": "5.2.1",
    "eslint": "9.32.0",
    "eslint-config-prettier": "10.1.8",
    "eslint-plugin-prettier": "5.5.3",
    "eslint-plugin-security": "3.0.1",
    "husky": "9.1.7",
    "mocha": "11.7.1",
    "mocha-junit-reporter": "2.2.1",
    "prettier": "3.6.2",
    "sinon": "21.0.0",
    "source-map-support": "0.5.21",
    "supertest": "7.1.4",
    "ts-loader": "9.5.2",
    "tsconfig-paths": "4.2.0",
    "tsx": "^4.20.3",
    "typescript": "5.8.3"
  }
}
