{
  "name": "milhas-api",
  "private": true,
  "version": "1.0.0",
  "description": "API for Milhas APP",
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "mocha --timeout 5000 --require ts-node/register './test/**/*.spec.ts'",
    "test:watch": "mocha --timeout 5000 --watch --require ts-node/register ./test/**/*.spec.ts",
    "test:cov": "nyc --reporter=lcov --reporter=cobertura mocha --timeout 5000 --reporter=cypress-multi-reporters --reporter-options configFile=cypress-multi-reporters.json --require ts-node/register --recursive './test/**/*.spec.ts'",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/mocha --runInBand",
    "test:single": "mocha --require ts-node/register ",
    "prepare": "husky install"
  },
  "dependencies": {
    "@nestjs/common": "^9.0.0",
    "@nestjs/config": "^2.2.0",
    "@nestjs/core": "^9.0.0",
    "@nestjs/jwt": "^9.0.0",
    "@nestjs/passport": "^9.0.0",
    "@nestjs/platform-express": "^9.0.0",
    "@nestjs/swagger": "^6.1.0",
    "@nhogs/nestjs-firebase": "^0.0.16",
    "bcrypt": "^5.0.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "jsonwebtoken": "^8.5.1",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.5.5"
  },
  "devDependencies": {
    "@nestjs/cli": "^9.0.0",
    "@nestjs/schematics": "^9.0.0",
    "@nestjs/testing": "^9.0.5",
    "@types/bcrypt": "^5.0.0",
    "@types/chai": "^4.3.3",
    "@types/express": "^4.17.13",
    "@types/jest": "^28.1.4",
    "@types/mocha": "^9.1.1",
    "@types/node": "^18.0.3",
    "@types/passport-jwt": "^3.0.6",
    "@types/passport-local": "^1.0.34",
    "@types/sinon": "^10.0.13",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.30.5",
    "@typescript-eslint/parser": "^5.30.5",
    "chai": "^4.3.6",
    "eslint": "^8.19.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-security": "^1.5.0",
    "husky": "^8.0.1",
    "mocha": "^10.0.0",
    "mocha-junit-reporter": "^2.0.2",
    "prettier": "^2.7.1",
    "sinon": "^14.0.0",
    "source-map-support": "^0.5.21",
    "supertest": "^6.2.4",
    "ts-loader": "^9.3.1",
    "ts-node": "^10.8.2",
    "tsconfig-paths": "^4.0.0",
    "typescript": "^4.7.4"
  }
}
